<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BlueHeader" parent="Widget.AppCompat.Toolbar">
        <item name="background">@color/colorPrimary</item>
    </style>

    <style name="TitleText" parent="Widget.AppCompat.TextView">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="electionButton" parent="Widget.AppCompat.Button">
        <item name="background">@color/colorPrimary</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="PrimaryButton" parent="Widget.AppCompat.Button">
        <item name="background">@color/colorPrimary</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="BlueSubText"></style>

    <style name="Link"></style>

    <style name="SubTitle"></style>
</resources>
